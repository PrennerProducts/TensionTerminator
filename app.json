{
  "expo": {
    "name": "TensionTerminator",
    "slug": "TensionTerminator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "tensionterminator",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mci.TensionTerminator",
      "infoPlist": {
        "NSCameraUsageDescription": "Tension Terminator App  needs to access your camera in order to evaluate your progress",
        "NSPhotoLibraryUsageDescription": "Just Camera needs to access your camera roll in order to allow you to chose the image of the promotion you would like to upload",
        "NSPhotoLibraryAddUsageDescription": "Just Camera needs to access your camera roll so that you can upload the image of the promotion of your choice"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mci.TensionTerminator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/favicon.png",
          "color": "#e30613"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "228f5cf4-c82d-4545-977b-6209737cd565"
      }
    }
  }
}
